{
  "name": "react-css-spinner",
  "version": "1.0.0",
  "description": "Initial setting up for all React Applications",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint '**/*.js'",
    "lint:fix": "prettier-eslint '**/*.js' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RonnieWang147/ReactProjectInit.git"
  },
  "keywords": [
    "React",
    "init",
    "settings"
  ],
  "author": "Ronnie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RonnieWang147/ReactProjectInit/issues"
  },
  "homepage": "https://github.com/RonnieWang147/ReactProjectInit#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.4.1",
    "lint-staged": "^13.1.0",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.2.0"
  },
  "engines": {
    "node": ">=8"
  }
}
